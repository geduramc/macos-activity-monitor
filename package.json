{
  "name": "macos-activity-monitor",
  "version": "1.0.0",
  "description": "CPU and Memory stats in your tray bar",
  "keywords": [
    "stats",
    "activity",
    "monitor",
    "cpu",
    "memory"
  ],
  "author": "Joel Mora <joelmora16@gmail.com>",
  "main": "index.js",
  "license": "MIT",
  "homepage": "./",
  "scripts": {
    "electron-build": "electron-packager . --platform=darwin --overwrite --icon=icons/icon.icns",
    "electron-start": "electron .",
    "create-icon": "png2icns icons/app-icon.png -s 16,32,64,128,256,512 -o icons/icon.icns",
    "react-start": "react-scripts start",
    "react-build": "yarn install && react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "build-app": "yarn react-build && yarn electron-build"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "chart.js": "^2.7.2",
    "chartjs-plugin-streaming": "^1.6.1",
    "electron-is-dev": "^1.0.1",
    "electron-store": "^2.0.0",
    "hex-rgb": "^3.0.0",
    "jimp": "^0.5.0",
    "menubar": "^5.2.3",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.7.4",
    "react-color": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^2.0",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "sweetalert2": "^7.28.2",
    "sweetalert2-react-content": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "electron": "^3.0.0",
    "electron-packager": "^12.1.2",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "png2icns": "^0.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
